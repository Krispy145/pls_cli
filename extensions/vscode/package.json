{
  "name": "lets-yak",
  "displayName": "Lets Yak",
  "publisher": "lets-yak-dev",
  "description": "vscode extension to streamline dev",
  "version": "0.8.26",
  "private": true,
  "icon": "assets/128.png",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "onCommand:lets-yak.create-app",
    "onCommand:lets-yak.create-package",
    "onCommand:lets-yak.create-ecosystem",
    "onCommand:lets-yak.add-feature",
    "onCommand:lets-yak.add-auth",
    "onCommand:lets-yak.add-data-layer",
    "onCommand:lets-yak.add-domain-layer",
    "onCommand:lets-yak.add-presentation-layer",
    "onCommand:lets-yak.add-structure",
    "onCommand:lets-yak.add-component",
    "onCommand:lets-yak.add-model",
    "onCommand:lets-yak.add-admob",
    "onCommand:lets-yak.add-notifications",
    "onCommand:lets-yak.add-deeplinks",
    "onCommand:lets-yak.add-assets",
    "onCommand:lets-yak.add-user-location",
    "onCommand:lets-yak.add-ecosystem-feature",
    "onCommand:lets-yak.add-ecosystem-presentation-layer",
    "onCommand:lets-yak.add-ecosystem-data-layer",
    "onCommand:lets-yak.add-ecosystem-domain-layer"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "submenus": [
      {
        "label": "Lets Yak",
        "id": "lets-yak.submenu"
      },
      {
        "label": "Ecosystem",
        "id": "lets-yak.ecosystem.submenu"
      }
    ],
    "commands": [
      {
        "command": "lets-yak.create-app",
        "title": "Lets Yak: Create a New App"
      },
      {
        "command": "lets-yak.create-package",
        "title": "Lets Yak: Create a New Package"
      },
      {
        "command": "lets-yak.create-ecosystem",
        "title": "Lets Yak: Create a New Ecosystem"
      },
      {
        "command": "lets-yak.add-feature",
        "title": "Add Feature",
        "shortTitle": "Add Feature"
      },
      {
        "command": "lets-yak.add-auth",
        "title": "Add Authentication",
        "shortTitle": "Add Authentication"
      },
      {
        "command": "lets-yak.add-data-layer",
        "title": "Add Data Layer",
        "shortTitle": "Add Data Layer"
      },
      {
        "command": "lets-yak.add-domain-layer",
        "title": "Add Domain Layer",
        "shortTitle": "Add Domain Layer"
      },
      {
        "command": "lets-yak.add-presentation-layer",
        "title": "Add Presentation Layer",
        "shortTitle": "Add Presentation Layer"
      },
      {
        "command": "lets-yak.add-structure",
        "title": "Add Structure",
        "shortTitle": "Add Structure"
      },
      {
        "command": "lets-yak.add-component",
        "title": "Add Component",
        "shortTitle": "Add Component"
      },
      {
        "command": "lets-yak.add-model",
        "shortTitle": "Add Model",
        "title": "Add Model"
      },
      {
        "command": "lets-yak.add-admob",
        "title": "Add Admob",
        "shortTitle": "Add AdMob"
      },
      {
        "command": "lets-yak.add-notifications",
        "title": "Add Notifications",
        "shortTitle": "Add Notifications"
      },
      {
        "command": "lets-yak.add-deeplinks",
        "title": "Add Deeplinks",
        "shortTitle": "Add Deeplinks"
      },
      {
        "command": "lets-yak.add-assets",
        "title": "Add Assets",
        "shortTitle": "Add Assets"
      },
      {
        "command": "lets-yak.add-user-location",
        "title": "Add User Location",
        "shortTitle": "Add User Location"
      },
      {
        "command": "lets-yak.add-ecosystem-feature",
        "title": "Add Feature",
        "shortTitle": "Add Feature"
      },
      {
        "command": "lets-yak.add-ecosystem-presentation-layer",
        "title": "Add Presentation Layer",
        "shortTitle": "Add Presentation Layer"
      },
      {
        "command": "lets-yak.add-ecosystem-data-layer",
        "title": "Add Data Layer",
        "shortTitle": "Add Data Layer"
      },
      {
        "command": "lets-yak.add-ecosystem-domain-layer",
        "title": "Add Domain Layer",
        "shortTitle": "Add Domain Layer"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "group": "01_flutter",
          "submenu": "lets-yak.submenu",
          "when": "explorerResourceIsFolder"
        }
      ],
      "commandPalette": [],
      "lets-yak.submenu": [
        {
          "submenu": "lets-yak.ecosystem.submenu",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "lets-yak.add-feature",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(lib))$/"
        },
        {
          "command": "lets-yak.add-auth",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(lib|presentation))$/"
        },
        {
          "command": "lets-yak.add-data-layer",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(lib|data))$/"
        },
        {
          "command": "lets-yak.add-domain-layer",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(lib|domain))$/"
        },
        {
          "command": "lets-yak.add-presentation-layer",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(lib|presentation))$/"
        },
        {
          "command": "lets-yak.add-structure",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(lib|navigation))$/"
        },
        {
          "command": "lets-yak.add-component",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(components|lib|presentation|navigation|utils|core|config|app))$/"
        },
        {
          "command": "lets-yak.add-model",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(models|domain|))$/"
        },
        {
          "command": "lets-yak.add-admob",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(dependencies|lib|))$/"
        },
        {
          "command": "lets-yak.add-notifications",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "lets-yak.add-deeplinks",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(dependencies|lib|))$/"
        },
        {
          "command": "lets-yak.add-assets",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(assets|lib|))$/"
        },
        {
          "command": "lets-yak.add-user-location",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(dependencies|lib|))$/"
        }
      ],
      "lets-yak.ecosystem.submenu": [
        {
          "command": "lets-yak.add-ecosystem-feature",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "lets-yak.add-ecosystem-presentation-layer",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "lets-yak.add-ecosystem-data-layer",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "lets-yak.add-ecosystem-domain-layer",
          "when": "explorerResourceIsFolder"
        }
      ]
    },
    "snippets": [
      {
        "language": "dart",
        "path": "./snippets/dart.json"
      }
    ],
    "languages": [
      {
        "id": "dart",
        "extensions": [
          ".dart"
        ],
        "aliases": [
          "Dart"
        ],
        "configuration": "./syntaxes/dart-language-configuration.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "build": "vsce package -o lets-yak.vsix",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4",
    "vsce": "^2.11.0"
  },
  "dependencies": {
    "change-case": "^5.0.2",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1",
    "ts-case-convert": "^2.0.7"
  }
}
