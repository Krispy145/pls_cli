{
  "name": "digital-oasis",
  "displayName": "Digital Oasis",
  "publisher": "skate-oasis",
  "description": "vscode extension to streamline dev",
  "version": "0.7.1",
  "repository": "https://github.com/Krispy145/digital-oasis-cli",
  "private": true,
  "icon": "assets/skate-oasis-circle.png",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:digital-oasis.create-app",
    "onCommand:digital-oasis.new-feature",
    "onCommand:digital-oasis.new-auth",
    "onCommand:digital-oasis.new-data-layer",
    "onCommand:digital-oasis.new-domain-layer",
    "onCommand:digital-oasis.new-presentation-layer",
    "onCommand:digital-oasis.new-structure",
    "onCommand:digital-oasis.add-component",
    "onCommand:digital-oasis.add-model",
    "onCommand:digital-oasis.add-admob",
    "onCommand:digital-oasis.add-notifications"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "submenus": [
      {
        "label": "Digital Oasis",
        "id": "digital-oasis.submenu"
      }
    ],
    "commands": [
      {
        "command": "digital-oasis.create-app",
        "title": "Digital Oasis: Create a New App"
      },
      {
        "command": "digital-oasis.new-feature",
        "title": "Feature",
        "shortTitle": "New Feature"
      },
      {
        "command": "digital-oasis.new-auth",
        "title": "Authentication",
        "shortTitle": "Add Authentication"
      },
      {
        "command": "digital-oasis.new-data-layer",
        "title": "Data Layer",
        "shortTitle": "New Data Layer"
      },
      {
        "command": "digital-oasis.new-domain-layer",
        "title": "Domain Layer",
        "shortTitle": "New Domain Layer"
      },
      {
        "command": "digital-oasis.new-presentation-layer",
        "title": "Presentation Layer",
        "shortTitle": "New Presentation Layer"
      },
      {
        "command": "digital-oasis.new-structure",
        "title": "Structure",
        "shortTitle": "New Structure"
      },
      {
        "command": "digital-oasis.add-component",
        "title": "Component",
        "shortTitle": "New Component"
      },
      {
        "command": "digital-oasis.add-model",
        "shortTitle": "New Model",
        "title": "Model"
      },
      {
        "command": "digital-oasis.add-admob",
        "title": "Add Admob",
        "shortTitle": "Add AdMob"
      },
      {
        "command": "digital-oasis.add-notifications",
        "title": "Add Notifications",
        "shortTitle": "Add Notifications"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "group": "01_flutter",
          "submenu": "digital-oasis.submenu",
          "when": "explorerResourceIsFolder"
        }
      ],
      "commandPalette": [],
      "digital-oasis.submenu": [
        {
          "command": "digital-oasis.new-feature",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(lib))$/"
        },
        {
          "command": "digital-oasis.new-auth",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(lib|presentation))$/"
        },
        {
          "command": "digital-oasis.new-data-layer",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(lib))$/"
        },
        {
          "command": "digital-oasis.new-domain-layer",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(lib))$/"
        },
        {
          "command": "digital-oasis.new-presentation-layer",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(lib))$/"
        },
        {
          "command": "digital-oasis.new-structure",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(lib|navigation))$/"
        },
        {
          "command": "digital-oasis.add-component",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(components|lib|presentation|navigation|utils|core|config|app))$/"
        },
        {
          "command": "digital-oasis.add-model",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(models|domain|))$/"
        },
        {
          "command": "digital-oasis.add-admob",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(dependencies|lib|))$/"
        },
        {
          "command": "digital-oasis.add-notifications",
          "when": "explorerResourceIsFolder && resource =~ /.*?(?<=(\\\\|\\/)(dependencies|lib|))$/"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "build": "vsce package -o digital-oasis.vsix",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4",
    "vsce": "^2.11.0"
  },
  "dependencies": {
    "change-case": "^5.0.2",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1"
  }
}
